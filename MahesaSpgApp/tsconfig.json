{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}